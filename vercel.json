{
    "version": 2,
    "framework": "nextjs",
    "buildCommand": "cd apps/frontend && npm install && npm run build",
    "outputDirectory": "apps/frontend/.next",
    "installCommand": "echo 'Skip root install'",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://your-engine-url.com/api/:path*"
        }
    ]
}