{
    "version": 2,
    "installCommand": "cd apps/frontend && npm install",
    "buildCommand": "cd apps/frontend && npm run build",
    "outputDirectory": "apps/frontend/.next",
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "https://your-engine-url.com/api/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/apps/frontend/.next/$1"
        }
    ]
}