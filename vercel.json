{
    "version": 2,
    "framework": "nextjs",
    "buildCommand": "npm install --prefix apps/frontend && npm run build --prefix apps/frontend",
    "outputDirectory": "apps/frontend/.next",
    "installCommand": "npm install --prefix apps/frontend",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://your-engine-url.com/api/:path*"
        }
    ]
}